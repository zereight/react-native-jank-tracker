{
  "name": "react-native-jank-tracker",
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "directories": {
    "example": "example",
    "test": "test"
  },
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    "./JankContext": {
      "types": "./dist/JankContext.d.ts",
      "default": "./dist/JankContext.js"
    },
    "./JankTrackerProvider": {
      "types": "./dist/JankTrackerProvider.d.ts",
      "default": "./dist/JankTrackerProvider.js"
    },
    "./useTTIMeasure": {
      "types": "./dist/useTTIMeasure.d.ts",
      "default": "./dist/useTTIMeasure.js"
    }
  },
  "scripts": {
    "test": "jest",
    "build": "tsc && node scripts/copy-dist-package.js",
    "build:example": "yarn build && cd example && yarn install",
    "ios:example": "yarn build:example && cd example && yarn ios",
    "android:example": "yarn build:example && cd example && yarn android",
    "example": "cd example && npx react-native run-ios",
    "e2e": "detox test",
    "start": "cd example && yarn start",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zereight/react-native-jank-tracker.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zereight/react-native-jank-tracker/issues"
  },
  "homepage": "https://github.com/zereight/react-native-jank-tracker#readme",
  "description": "",
  "dependencies": {
    "react": "^19.0.0",
    "react-native": "^0.79.2",
    "use-context-selector": "^2.0.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.0",
    "babel-jest": "^29.7.0",
    "detox": "^20.0.0",
    "eslint-plugin-prettier": "^5.3.1",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "react-test-renderer": "^19.0.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  }
}
